    "terminal": {
      "title": "CLI",
      "content": "<div class=\"terminal\"><div class=\"terminal-line\">WebGUI CLI</div><div class=\"terminal-line\">(c) 2025 CyborgsDream</div><div class=\"terminal-line\">WebGUI started.</div><div class=\"terminal-line\">System date: {DATE}</div><div class=\"terminal-line\">System time: {TIME}</div><div class=\"terminal-line\"><br></div><div class=\"terminal-line\"><span class=\"prompt\">1&gt;</span> <span class=\"cursor\">‚ñà</span></div></div>",
      "width": 450,
