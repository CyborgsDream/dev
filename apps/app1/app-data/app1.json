    "text-editor": {
      "title": "Text Editor",
      "content": "<div class=\"text-editor\" style=\"display:flex;flex-direction:column;height:100%\"><div style=\"text-align:right;margin-bottom:5px;\"><button id=\"text-editor-export\" style=\"font-size:16px;\">Export PDF</button></div><textarea id=\"text-editor-area\" style=\"flex:1;background:#fff;border:1px solid #aaa;padding:5px;font-family:monospace;font-size:18px;\">Welcome to WebGUI 0.0.18!\n\nThe icon issue has been fixed!\n‚Ä¢ All desktop icons are now fully functional\n‚Ä¢ Click any icon to open an application\n‚Ä¢ The wallpaper no longer blocks clicks\n\nTry clicking on any icon to open its application.\n\nWebGUI continues to push the boundaries of personal computing!</textarea></div>",
      "width": 500,
      "height": 350
    },
    "graphics": {
      "title": "WebGUI Paint",
      "content": "<div style=\"text-align: center;\"><h3>WebGUI Paint v2.0</h3><div style=\"margin: 10px auto; width: 300px; height: 200px; background: #fff; border: 1px solid #000; position: relative;\"><div style=\"position: absolute; top: 20px; left: 30px; width: 100px; height: 100px; background: #f00;\"></div><div style=\"position: absolute; top: 50px; left: 70px; width: 60px; height: 60px; background: #00f;\"></div><div style=\"position: absolute; top: 70px; left: 40px; width: 40px; height: 40px; background: #ff0;\"></div><div style=\"position: absolute; top: 120px; left: 100px; width: 80px; height: 40px; background: #0f0;\"></div></div><div style=\"display: flex; justify-content: center; gap: 10px; margin-top: 15px;\"><button style=\"padding: 5px 10px; font-size: 18px;\">Pencil</button><button style=\"padding: 5px 10px; font-size: 18px;\">Brush</button><button style=\"padding: 5px 10px; font-size: 18px;\">Fill</button></div></div>",
      "width": 400,
      "height": 300
    },
    "utilities": {
      "title": "Utilities",
      "content": "<h3>System Utilities</h3><ul style=\"margin-top: 15px; margin-left: 20px; font-size: 18px;\"><li>Disk Formatter</li><li>Memory Viewer</li><li>System Monitor</li><li>Printer Setup</li><li>Serial Port Config</li><li>Network Setup</li></ul><div style=\"margin-top: 20px; padding: 10px; background: #ddd; border: 1px solid #aaa; font-size: 16px;\"><strong>Note:</strong> Some utilities require system disk.</div>",
      "width": 300,
      "height": 300
    },
    "games": {
      "title": "Games",
      "content": "<h3>WebGUI Games Collection</h3><div style=\"display: grid; grid-template-columns: repeat(3, 1fr); gap: 10px; margin-top: 15px; font-size: 16px;\"><div style=\"background: #aaccff; padding: 5px; text-align: center;\">Defender of the Crown</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">Marble Madness</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">The Bard's Tale</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">Leisure Suit Larry</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">SimCity</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">Battle Chess</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">Turrican</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">Lemmings</div><div style=\"background: #aaccff; padding: 5px; text-align: center;\">Shadow of the Beast</div></div><p style=\"margin-top: 15px; font-size: 18px;\"><em>Insert game disk to play...</em></p>",
      "width": 500,
      "height": 300
